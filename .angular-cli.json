{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "4iiz-process-maker"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "push-service-worker.js"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/sweetalert2/dist/sweetalert2.min.css",
        "./assets/css/material-dashboard.css",
        "./assets/css/styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/sweetalert2/dist/sweetalert2.min.js",
        "../node_modules/chart.js/dist/Chart.min.js",
        "../node_modules/arrive/src/arrive.js",
        "../node_modules/moment/moment.js",
        "../node_modules/moment-timezone/builds/moment-timezone.min.js",
        "../node_modules/twitter-bootstrap-wizard/jquery.bootstrap.wizard.js",
        "../node_modules/bootstrap-notify/bootstrap-notify.js",
        "../node_modules/eonasdan-bootstrap-datetimepicker/src/js/bootstrap-datetimepicker.js",
        "../node_modules/nouislider/distribute/nouislider.min.js",
        "../node_modules/bootstrap-select/dist/js/bootstrap-select.js",
        "../node_modules/datatables/media/js/jquery.dataTables.js",
        "../node_modules/fullcalendar/dist/fullcalendar.js",
        "../node_modules/bootstrap-tagsinput/dist/bootstrap-tagsinput.js",
        "../node_modules/crypto-js/crypto-js.js",
        "../node_modules/auth0-js/build/auth0.min.js",
        "../node_modules/jquery-mask-plugin/dist/jquery.mask.min.js",
        "../src/assets/js/core/material.min.js",
        "../src/assets/js/plugins/jquery-jvectormap.js",
        "../src/assets/js/core/jquery.perfect-scrollbar.min.js",
        "../src/assets/js/core/jquery.validate.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}